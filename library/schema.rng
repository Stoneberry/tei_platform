               <data type="string"/>
            </attribute>
         </optional>
         <attribute name="type">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Classifies the segment</a:documentation>
            <choice>
               <value>act</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Act, also "Aufzug", "Auftritt"</a:documentation>
               <value>Dramatis_Personae</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Dramatis personae</a:documentation>
               <value>scene</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Scene</a:documentation>
               <value>subscene</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Subscene</a:documentation>
               <value>dedication</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Dedication</a:documentation>
               <value>preface</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Preface</a:documentation>
               <value>postface</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Postface</a:documentation>
               <value>prologue</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Prologue</a:documentation>
               <value>epilogue</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Prologue</a:documentation>
               <value>interlude</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Interlude, auch: Zwischenspiel</a:documentation>
               <value>entracte</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Entracte</a:documentation>
               <value>ouverture</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Ouverture</a:documentation>
               <value>location</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Location change</a:documentation>
               <value>configuration</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Change of character constellation, which is not marked as scene change</a:documentation>
               <value>part</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Part</a:documentation>
               <value>appendix</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Appendix</a:documentation>
               <value>tableau</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Tableau, auch: Kartina</a:documentation>
               <value>dictionary</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Dictionary</a:documentation>
               <value>variant</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Variant</a:documentation>
               <value>front</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Front [Title, Author,...]</a:documentation>
               <value>epigraph</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Epigraph</a:documentation>
               <value>unten</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">"unten"</a:documentation>
               <value>oben</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">"oben"</a:documentation>
               <value>cholerisch</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">"cholerisch"</a:documentation>
               <value>phlegmatisch</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">"phlegmatisch"</a:documentation>
               <value>melancholisch</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">"melancholisch"</a:documentation>
               <value>sanguinisch</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">"sanguinisch"</a:documentation>
            </choice>
         </attribute>
         <empty/>
      </element>
   </define>
   <define name="tei_trailer">
